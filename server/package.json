{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project for building Express applications using TypeScript. Includes common middleware (CORS, Helmet, Morgan), rate limiting, and slowdown capabilities, with a development setup using ts-node-dev.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn -- app.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Ksense Technology Group",
  "license": "UNLICENSED",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "express-slow-down": "^1.6.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "husky": "^8.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
