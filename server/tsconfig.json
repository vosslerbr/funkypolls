{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
